<sl-object xsi:noNamespaceSchemaLocation="../../xsd/sl-object.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <title>Simple Voter</title>
    <description>
        Simple voting system, consisting of a green, yellow, orange and red voter object. 
        Note that this XML description describes only one of those four voring instances. 
    </description>
    <creator>
        <name>Benno Schmidt</name>
        <sl-id>BennoSchmidt</sl-id>
    </creator>
    <commands>
        <command>reset</command>
        <command>show</command>     
        <command>hide</command>
        <command>float</command>
        <command>nofloat</command>
        <command>list</command>
        <command>help</command>
    </commands>
    <primitives>
        <sl-prim>Prism</sl-prim>
    </primitives>
    <content>
        <sl-script>/scripts/Voter.lsl</sl-script>
    </content>
</sl-object>
